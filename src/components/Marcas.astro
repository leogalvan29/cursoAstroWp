---
const token = import.meta.env.TOKEN

const response = await fetch('https://programadorculiacan.com/wp-json/wp/v2/logos?acf_format=standard' ,{
	method:'GET',
	headers:{
		'Authorization':`Bearer ${token}`, 
		'Content-Type':'application/json'
	}
}) 
const data = await response.json()  
//console.log(data)


---


<!-- Logo cloud -->
<div class="mx-auto max-w-7xl px-6 lg:px-8 py-[50px]">
    <div class="mx-auto grid max-w-lg grid-cols-4 items-center gap-x-8 gap-y-12 sm:max-w-xl sm:grid-cols-6 sm:gap-x-10 sm:gap-y-14 lg:mx-0 lg:max-w-none lg:grid-cols-5">

        {data.map((logo) => ( 

                 <img class="col-span-2 max-h-12 w-full object-contain lg:col-span-1" src={logo.acf.logo.url} alt="Transistor" width="158" height="48">
        ))}
 
      
    </div>
  </div>